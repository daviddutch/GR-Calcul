@model GR_Calcul.Models.Course

@{
    ViewBag.Title = "Supprimer";
}

<h2>Supprimer un cours</h2>

<h3>Etes vous sur de voir suprimer ce cours ?</h3>
@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

     @Html.HiddenFor(model => model.ID)
     <input type="hidden" id="Timestamp" name="Timestamp" value="@Model.Timestamp" />

    <p><b>Nom : </b>@Model.Name</p>
    <p><b>Responsable : </b>@Model.Responsible</p>
    <p><b>Clefs : </b>@Model.Key</p>


    <p>
        <input type="submit" value="Supprimer" /> |
        @Html.ActionLink("Retour à la liste", "Index")
    </p>
}


